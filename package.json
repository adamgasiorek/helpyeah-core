{
  "name": "helpyeah",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --localize",
    "lint": "ng lint",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "int:extract": "ng xi18n helpyeah --format xlf --output-path src/locale && ng run helpyeah:xliffmerge",
    "e2e:ui": "cypress open",
    "e2e:cli": "cypress run",
    "run:android": "npm run build && npx cap update && npx cap copy && npx cap open android",
    "analyze": "ng build --statsJson && webpack-bundle-analyzer dist/browser/stats-es2015.json ",
    "build:ssr": "npm run build:prod && ng run helpyeah:server:production",
    "predeploy": "npm --prefix functions run build && rm -rf ./dist/browser/index.html",
    "deploy:functions": "npm --prefix functions run build && firebase deploy --only functions",
    "deploy:manual:ssr:prod": "npm run build:ssr && npm run predeploy && firebase deploy -P production --only hosting,functions:ssr",
    "deploy:manual:ssr:dev": "npm run build:ssr && npm run predeploy && firebase deploy -P dev --only hosting,functions:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "9.1.6",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/platform-server": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@angular/service-worker": "~9.1.1",
    "@capacitor/android": "^2.0.1",
    "@capacitor/cli": "^2.0.1",
    "@capacitor/core": "^2.0.1",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@ngxs-labs/dispatch-decorator": "^2.1.0",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "algoliasearch": "^3.35.1",
    "angular-instantsearch": "^3.0.0-beta.5",
    "express": "^4.15.2",
    "firebase": "^7.14.1",
    "instantsearch.js": "3.7.0",
    "ng-click-outside": "^6.0.0",
    "ngx-stripe": "^9.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nguniversal/builders": "^9.1.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@types/algoliasearch": "^3.34.10",
    "@types/express": "^4.17.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "cypress": "^4.4.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^25.3.0",
    "jest-preset-angular": "^8.1.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./src/test.ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@shared/(.*)": "<rootDir>/src/app/shared/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@modules/(.*)": "<rootDir>/src/app/modules/$1"
    },
    "testPathIgnorePatterns": [
      "./src/test.ts",
      "./cypress/"
    ]
  }
}
